{
  "name": "web-scraper-app",
  "version": "1.0.0",
  "description": "Deployable web scraper with browser-like feel and mobile support",
  "type": "module",
  "scripts": {
    "install-all": "npm install && npm --prefix server install && npm --prefix client install",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm --prefix server run dev",
    "dev:client": "npm --prefix client run dev",
    "build:server": "npm --prefix server run build",
    "build:client": "npm --prefix client run build",
    "build": "npm run build:client && npm run build:server",
    "start:server": "npm --prefix server start",
    "docker:build": "docker build -t web-scraper-app .",
    "docker:run": "docker run -p 5000:5000 -p 3000:3000 web-scraper-app"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.1",
    "@types/puppeteer": "^5.4.7",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "cheerio": "^1.2.0",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "puppeteer": "^24.37.5"
  },
  "main": "test_python_engine.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kvngz3n0/codespaces-blank.git"
  },
  "keywords": [],
